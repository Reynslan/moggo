{
		"name": "Moggo",
		"author": "Reynslan",
		"version": "1.0.0",
		"manifest_version": 2,
		"description": "A browser extension for mbl.is",
		"background": {
			"scripts": [
				"background.js"
			]
		},
		"content_scripts": [
			{
				"matches": [
					"*://*.mbl.is/*"
				],
				"js": [
						"thirdparty/react.js",
						"thirdparty/routie.js",
						"thirdparty/sizzle.js",
						"thirdparty/lodash.js",
						"BabelExt.js",
						"extension.js"],
						"run_at": "document_end"
			}
		],
		"web_accessible_resources": [
			"graphics/*.png"
		],
		"icons": {},
		"permissions": [
			"*://*.mbl.is/*",
			"contextMenus",
			"tabs",
			"history",
			"notifications",
			"storage"
		]
}