{
		"name": "Mogg√≥",
		"author": "Reynslan",
		"version": "1.0.1",
		"manifest_version": 2,
		"description": "Enhance mbl.is",
		"background": {
			"scripts": [
				"background.js"
			]
		},
		"content_scripts": [
			{
				"matches": [
					"*://*.mbl.is/*"
				],
				"js": [
						"thirdparty/routie.js",
						"BabelExt.js",
						"extension.js"],
						"run_at": "document_end"
			}
		],
		"web_accessible_resources": [
			"graphics/*.png"
		],
		"icons": {
			"48": "icon48.png",
			"128": "icon128.png"
		},
		"permissions": [
			"*://*.mbl.is/*",
			"contextMenus",
			"tabs",
			"history",
			"notifications",
			"storage"
		]
}